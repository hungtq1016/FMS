{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Authenticate/reset-password",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/auth/reset-password",
      "UpstreamHttpMethod": [ "Post" ],
      "SwaggerKey": "auth"
    },
    {
      "DownstreamPathTemplate": "/api/Authenticate/login",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/auth/login",
      "UpstreamHttpMethod": [ "Post" ],
      "SwaggerKey": "auth"
    },
    {
      "DownstreamPathTemplate": "/api/Roles",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5003
        }
      ],
      "UpstreamPathTemplate": "/roles",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "role"
    },
    {
      "DownstreamPathTemplate": "/api/Roles/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5003
        }
      ],
      "UpstreamPathTemplate": "/roles/{id}",
      "UpstreamHttpMethod": [ "Put", "Delete", "Get" ],
      "SwaggerKey": "role"
    },
    {
      "DownstreamPathTemplate": "/api/Users",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5005
        }
      ],
      "UpstreamPathTemplate": "/users",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "user"
    },
    {
      "DownstreamPathTemplate": "/api/Users/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5005
        }
      ],
      "UpstreamPathTemplate": "/users/{id}",
      "UpstreamHttpMethod": [ "Put", "Delete", "Get" ],
      "SwaggerKey": "user"
    },
    {
      "DownstreamPathTemplate": "/api/Roles/ByUserId/{userId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5003
        }
      ],
      "UpstreamPathTemplate": "/roles/user/{userId}",
      "UpstreamHttpMethod": [ "Get" ],
      "Key": "roles",
      "SwaggerKey": "role"
    },
    {
      "DownstreamPathTemplate": "/api/Users/{userId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5005
        }
      ],
      "UpstreamPathTemplate": "/users/info/{userId}",
      "UpstreamHttpMethod": [ "Get" ],
      "Key": "user"
    }
  ],
  "Aggregates": [
    {
      "RouteKeys": [
        "user",
        "roles"
      ],
      "UpstreamPathTemplate": "/user-detail/{userId}",
      "SwaggerKey": "user"
    }
  ],

  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5000"
  },
  "SwaggerEndPoints": [
    {
      "Key": "auth",
      "Config": [
        {
          "Name": "Authenticate API",
          "Version": "v1",
          "Url": "https://localhost:5001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "role",
      "Config": [
        {
          "Name": "Role API",
          "Version": "v1",
          "Url": "https://localhost:5003/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "user",
      "Config": [
        {
          "Name": "User API",
          "Version": "v1",
          "Url": "https://localhost:5005/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}
